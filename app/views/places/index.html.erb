<div class="container-fluid">
  <div class="row">
    <h1>My Places</h1>
    <div class="row">
      <ul>
        <% (@places || []).each_with_index do |place, index| %>
          <li class="col-sm-2">
            <%= place.address %>
            <%= link_to "delete", place_path(place.id), :method => :delete %>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
