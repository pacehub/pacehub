<% content_for :additional_head do %>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.2/underscore-min.js"></script>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC62LADU--Wasiae3_LGpkvaVIUlYfJ1EU&callback=initMap"
  async defer></script>
<% end %>

<div class="container-fluid">
  <div class="row">
    <div class="col-sm-2"></div>
    <div class="col-sm-8">
      <h2 style="text-align:center">We are still in Beta. Sign up for early access!</h2>
    </div>
    <div class="col-sm-2"></div>
  </div>
</div>

<div class="container-fluid">
  <div class="row">
    <div class="col-sm-1"></div>
    <div class="col-sm-10">
      <%= content_for :map %>
      <%= render 'maps/map' %>
    </div>
    <div class="col-sm-1"></div>
  </div>
</div>
